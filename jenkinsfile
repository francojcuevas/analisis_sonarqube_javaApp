pipeline {

    agent any
    stages {
   
        stage('Build') {
            steps {
              sh 'mvn clean package -DskipTest=true'   
            }
        }                        
        stage('SonarQube analysis') {
            steps {
		withSonarQubeEnv('sonarqube-8.9.2') {
		sh "mvn sonar:sonar"
		}
            }
        }

   	}
}

