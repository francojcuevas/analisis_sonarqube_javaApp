pipeline {

    agent any
    tools { 
      maven 'MAVEN_HOME' 
      jdk 'JAVA_HOME' 
    }
    stages {
        stage('Build') {
	steps{
              sh 'mvn clean package'   
	    }
        }                        
        stage('SonarQube analysis') {
            steps {
		withSonarQubeEnv('sonarqube-8.9.2') {
		sh 'mvn sonar:sonar'
		}
            }
        }

   	}
}

