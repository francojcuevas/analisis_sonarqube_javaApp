pipeline {

    agent any
    stages {
	stage('SCM Checkout') {
    	git 'https://github.com/francojcuevas/analisis_sonarqube_javaApp.git'
	}

        stage('Build') {
            steps {
              sh 'mvn clean package'   
            }
        }                        
        stage('SonarQube analysis') {
            steps {
		withSonarQubeEnv('sonarqube-8.9.2') {
		sh "mvn sonar:sonar"
   
		}
            }
        }

   	}
}

