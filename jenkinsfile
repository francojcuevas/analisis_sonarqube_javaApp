pipeline {

    agent any
    stages {

        stage('Build') {
            steps {
                sh 'mvn package'   
            }
        }                        
        stage('SonarQube analysis) {
            steps {
		withSonarQubeEnv(credentialsId: 'token_sonar') {
		sh "mvn sonar:sonar"
   
		}
            }
        }

   	}
}

