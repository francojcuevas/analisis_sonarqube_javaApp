pipeline {

    agent any
    stages {
   
        stage('Build') {
            withMaven {
              sh "mvn clean package"   
            }
        }                        
        stage('SonarQube analysis') {
            steps {
		withSonarQubeEnv('sonarqube-8.9.2') {
		sh "mvn sonar:sonar"
		}
            }
        }

   	}
}

